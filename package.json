{
  "name": "aidivalogger",
  "version": "1.0.0",
  "description": "Neptune Apex aquarium controller data logger for InfluxDB",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@influxdata/influxdb3-client": "^2.0.0",
    "dotenv": "^16.4.5",
    "fast-xml-parser": "^4.5.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/node-cron": "^3.0.11",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
